local ASC11LS = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local TextLabel = Instance.new("TextLabel")
local TextButton = Instance.new("TextButton")
local UICorner = Instance.new("UICorner")

-- Properties

ASC11LS.Name = "ASC1.1LS"
ASC11LS.Parent = game.Workspace
ASC11LS.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

Frame.Parent = ASC11LS
Frame.BackgroundColor3 = Color3.new(0.603922, 0.117647, 1)
Frame.Position = UDim2.new(0, -36, 0, -122)
Frame.Size = UDim2.new(0, 1364, 0, 1006)

TextLabel.Parent = Frame
TextLabel.BackgroundColor3 = Color3.new(0, 0, 0)
TextLabel.BackgroundTransparency = 1
TextLabel.Position = UDim2.new(0, 450, 0, 197)
TextLabel.Size = UDim2.new(0, 495, 0, 87)
TextLabel.Font = Enum.Font.Unknown
TextLabel.Text = "Loading Aristois Client V1.1"
TextLabel.TextColor3 = Color3.new(0, 0, 0)
TextLabel.TextScaled = true
TextLabel.TextSize = 14
TextLabel.TextStrokeColor3 = Color3.new(0.988235, 0.709804, 0.54902)
TextLabel.TextWrapped = true

TextButton.Parent = Frame
TextButton.BackgroundColor3 = Color3.new(0.109804, 0.466667, 1)
TextButton.Position = UDim2.new(0, 1063, 0, 608)
TextButton.Size = UDim2.new(0, 200, 0, 50)
TextButton.Font = Enum.Font.Unknown
TextButton.Text = "Skip"
TextButton.TextColor3 = Color3.new(0, 0, 0)
TextButton.TextSize = 33

UICorner.Parent = TextButton

-- Scripts

local function LVNZK_fake_script() -- Frame.Script 
	local script = Instance.new('Script', Frame)

	local button = script.Parent:WaitForChild("TextLabel")
	button.TextTransparency = 1
	wait(1)
	button.TextTransparency = 0
	wait(1)
	button.TextTransparency = 1
	wait(1)
	button.TextTransparency = 0
	wait(1)
	button.TextTransparency = 1
	wait(1)
	button.TextTransparency = 0
	wait(1)
	button.TextTransparency = 1
	wait(1)
	button.TextTransparency = 0
	wait(1)
	button.TextTransparency = 1
	wait(1)
	button.TextTransparency = 0
	wait(1)
	button.TextTransparency = 1
	wait(1)
	button.TextTransparency = 0
	wait(1)
	button.TextTransparency = 1
	wait(1)
	button.TextTransparency = 0
	wait(1)
	button.TextTransparency = 1
	wait(1)
	button.TextTransparency = 0
	wait(1)
	button.TextTransparency = 1
	wait(1)
	button.TextTransparency = 0
	wait(1)
	button.TextTransparency = 1
	wait(1)
	button.TextTransparency = 0
	wait(1)
	button.TextTransparency = 1
end
coroutine.wrap(LVNZK_fake_script)()
local function RFRV_fake_script() -- Frame.Script 
	local script = Instance.new('Script', Frame)

	wait(18.5)
	script.Parent.Visible = false
end
coroutine.wrap(RFRV_fake_script)()
local function ZECGM_fake_script() -- TextButton.Script 
	local script = Instance.new('Script', TextButton)

	script.Parent.MouseButton1Down:Connect(function()
		script.Parent.Parent.Visible = false
	end)
	
end
coroutine.wrap(ZECGM_fake_script)()
